generator cliente {
  provider = "prisma-client-js"
}

datasource bd {
  provider = "postgresql"
  url      = env("BD_URL")
}

model Regimen {
  codigo String  @id @unique
  nombre String  @unique
  casos  Casos[]
}

model Etnia {
  codigo String @id @unique
  nombre String  @unique
  casos  Casos[]
}

model Caracterizacion {
  id     Int     @id @default(autoincrement())
  nombre String  @unique
  casos  Casos[]
}

model Casos {
  id                 Int             @id @default(autoincrement())
  anno               Int
  departamentoCodigo String
  municipioCodigo    String
  etniaCodigo        String
  etnia              Etnia           @relation(fields: [etniaCodigo], references: [codigo])
  regimenCodigo      String
  regimen            Regimen         @relation(fields: [regimenCodigo], references: [codigo])
  sexo               String
  caracterizacionId  Int
  caracterizacion    Caracterizacion @relation(fields: [caracterizacionId], references: [id])
  numerador          Int
  denominador        Int
  porcentaje         Float
}
